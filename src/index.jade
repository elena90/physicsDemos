doctype html
html(lang="ru") 
  head
    meta(charset="utf-8")
    title PhysicsJS
    link(href="/css/build.min.css", rel="stylesheet")
    
  body

    - var listOfExamples = [];
    - listOfExamples.push({title: 'Движение мячика', link: '/examples/01_movingBall/'})
    - listOfExamples.push({title: 'Сила трения', link: '/examples/02_frictionBall/'})
    - listOfExamples.push({title: 'Сила трения пример (горизонтальный)', link: '/examples/03_frictionForceHoris/'})
    - listOfExamples.push({title: 'Сила трения пример (вертикальный)', link: '/examples/04_frictionForceVert/'})
    - listOfExamples.push({title: 'Сила трения пример (со стрелками)', link: '/examples/05_frictionForceWithArrows/'})
    - listOfExamples.push({title: 'Ускоряющийся мячик', link: '/examples/06_acceleratingBall/'})
    - listOfExamples.push({title: 'Прыгающий мячик', link: '/examples/06_bouncingBall/'})
    - listOfExamples.push({title: 'Прыгающая галерея', link: '/examples/06_galleryBouncing/'})
    - listOfExamples.push({title: 'Соударение', link: '/examples/07_collisionBalls/'})
    - listOfExamples.push({title: 'Эффект соударения', link: '/examples/08_collisionEffect/'})
    - listOfExamples.push({title: 'Колебание', link: '/examples/09_spring/'})
    - listOfExamples.push({title: 'Колебание по окружности', link: '/examples/10_springRound/'})
    - listOfExamples.push({title: 'Колебание затухающее', link: '/examples/11_springDamping/'})
    - listOfExamples.push({title: 'Логотип', link: '/examples/12_logo/'})
    - listOfExamples.push({title: 'Логотипы', link: '/examples/13_logos/'})
    - listOfExamples.push({title: 'Оттягивание окон', link: '/examples/14_windowSpring/'})
    - listOfExamples.push({title: 'Собирающаяся картинка', link: '/examples/15_image/'})
    - listOfExamples.push({title: 'Распад', link: '/examples/16_movingObjects/'})
    - listOfExamples.push({title: 'Взрыв', link: '/examples/17_explosion/'})


    ul.listOfExamples
      each item in listOfExamples
        li.listOfExamples__item
          a.listOfExamples__link(href=item.link)!= item.title